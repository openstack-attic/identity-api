            <param name="per_page" style="query" type="xsd:string" required="false">
                <wadl:doc xmlns="http://docbook.org/ns/docbook">
                    Defaults to 30.
                </wadl:doc>
            </param>
        </request>
        <!--No request body is provided-->
        <response status="200">
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook">
                    <xsdxt:sample title="List Services Response: JSON">
                        <xsdxt:code href="../common/samples/ServicesListResponse.json"/>	
                    </xsdxt:sample>
                </wadl:doc>
            </representation>
        </response>
        &commonFaults;
        &getFaults;
    </method>
    
