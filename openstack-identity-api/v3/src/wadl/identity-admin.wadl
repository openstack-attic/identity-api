                    </resource>
                </resource>                
                <resource id="projectIDGroupIDRoles" path="groups/{group_id}/roles">
                    <param name="group_id" style="template" type="xsd:string" required="true">
                        <doc>Group to be acted upon</doc>
                    </param>
                    <method href="#listProjectGroupRoles"/>
                </resource>
                <resource id="projectIDGroupIDRoleID" path="{role_id}">
                    <param name="role_id" style="template" type="xsd:string" required="true">
                        <doc>Role to be acted upon</doc>
                    </param>
                    <method href="#grantProjectGroupRole"/>
                    <method href="#checkProjectGroupRole"/>
                    <method href="#revokeProjectGroupRole"/>
                </resource>
            </resource>
        </resource>
        
        <resource id="users" path="//v3/users">
            <method href="#listUsers"/>
            <method href="#createUser"/>
            <resource id="userID" path="{user_id}">
                <param name="user_id" style="template" type="xsd:string" required="true">
                    <doc>User to be acted upon</doc>
                </param>
                <method href="#getUser"/>
                <method href="#updateUser"/>
                <method href="#deleteUser"/>
                <resource id="userIDGroups" path="groups">
                    <method href="#listUserGroups"/>
                </resource>
                <resource id="userIDProjects" path="projects">
                    <method href="#listUserProjects"/>
                </resource>
                <resource id="userIDRoles" path="roles">
                    <method href="#listUserRoles"/>
                </resource>
            </resource>
        </resource>
        
        <resource id="groups" path="//v3/groups">
            <method href="#listGroups"/>
            <method href="#createGroup"/>
            <resource id="groupID" path="{group_id}">
                <param name="group_id" style="template" type="xsd:string" required="true">
                    <doc>Group to be acted upon</doc>
                </param>
                <method href="#getGroup"/>
                <method href="#updateGroup"/>
                <method href="#deleteGroup"/>
                <resource id="groupIDUsers" path="users">
                    <method href="#listGroupUsers"/>
                    <resource id="groupIDUserIDs" path="{user_id}">
                        <method href="#addGroupUser"/>
                        <method href="#deleteGroupUser"/>
                        <method href="#checkGroupUser"/>
                    </resource>
                </resource>
            </resource>
        </resource>

        <resource id="credentials" path="//v3/credentials">
            <method href="#listCredentials"/>
            <method href="#createCredential"/>
            <resource id="credentialID" path="{credential_id}">
                <param name="credential_id" style="template" type="xsd:string" required="true">
                    <doc>Credential to be acted upon</doc>
                </param>
                <method href="#getCredential"/>
                <method href="#updateCredential"/>
                <method href="#deleteCredential"/>
            </resource>
        </resource>
        
        <resource id="roles" path="//v3/roles">
            <method href="#listRoles"/>
            <method href="#createRole"/>
            <resource id="roleID" path="{role_id}">
                <param name="role_id" style="template" type="xsd:string" required="true">
                    <doc>Role to be acted upon</doc>
                </param>
                <method href="#getRole"/>
                <method href="#updateRole"/>
                <method href="#deleteRole"/>
                <resource id="roleIDUsers" path="users">
                    <method href="#listRoleUsers"/>
                </resource>
            </resource>
        </resource>
        
        <resource id="policies" path="//v3/policies">
            <method href="#listPolicies"/>
            <method href="#createPolicy"/>
            <resource id="policyID" path="{policy_id}">
                <param name="policy_id" style="template" type="xsd:string" required="true">
                    <doc>Policy to be acted upon</doc>
                </param>
                <method href="#getPolicy"/>
                <method href="#updatePolicy"/>
                <method href="#deletePolicy"/>
            </resource>
        </resource>
