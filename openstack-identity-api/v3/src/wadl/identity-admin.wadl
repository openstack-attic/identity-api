        <!-- FYI: double slash needed in path to get a single slash to display -->
        <resource id="versions" path="//v3/versions">
            <method href="#listVersions"/>
        </resource>

        <resource id="tokens" path="//v3/tokens">
            <method href="#authenticateTokens"/> 
            <method href="#validateTokens"/>
            <method href="#checkTokens"/>
            <method href="#revokeTokens"/>                   
        </resource>

        <resource id="catalog" path="//v3/services">
            <method href="#listServices"/>
            <method href="#createService"/>
                <resource id="catalogID" path="{service_id}">
                    <param name="service_id" style="template" type="xsd:string" required="true">
                        <doc>Service to be acted upon</doc>
                    </param>
                    <method href="#getService"/>
                    <method href="#updateService"/>
                    <method href="#deleteService"/>
                </resource>
        </resource>
        
        <resource id="endpoints" path="//v3/endpoints">
            <method href="#listEndpoints"/>
            <method href="#createEndpoint"/>
            <resource id="endpointsID" path="{endpoint_id}">
                <param name="endpoint_id" style="template" type="xsd:string" required="true">
                    <doc>Endpoint to be acted upon</doc>
                </param>
                <method href="#updateEndpoint"/>
                <method href="#deleteEndpoint"/>
            </resource>
        </resource>
        
        <resource id="domains" path="//v3/domains">
            <method href="#listDomains"/>
            <method href="#createDomain"/>
            <resource id="domainID" path="{domain_id}">
