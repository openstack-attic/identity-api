    <method name="GET" id="getService">
        <wadl:doc xml:lang="EN" xmlns="http://docbook.org/ns/docbook" title="Get Service">
            <para class="shortdesc">Get Service.</para>
            <para>This needs detail to be provided by Identity Team.</para>
