                    <doc>Domain to be acted upon</doc>
                </param>
                <method href="#getDomain"/>
                <method href="#updateDomain"/>
                <method href="#deleteDomain"/>
                <resource id="domainIDUserIDRoles" path="users/{user_id}/roles">
                    <param name="user_id" style="template" type="xsd:string" required="true">
                        <doc>User to be acted upon</doc>
                    </param>
                    <method href="#listDomainUserRoles"/>
                    <resource id="domainIDUserIDRoleID" path="{role_id}">
                        <param name="role_id" style="template" type="xsd:string" required="true">
                            <doc>Role to be acted upon</doc>
                        </param>
                        <method href="#grantDomainUserRole"/>
                        <method href="#checkDomainUserRole"/>
                        <method href="#revokeDomainUserRole"/>
                    </resource>
                </resource>
                <resource id="domainIDGroupIDRoles" path="groups/{group_id}/roles">
                    <param name="group_id" style="template" type="xsd:string" required="true">
                        <doc>Group to be acted upon</doc>
                    </param>
                    <method href="#listDomainGroupRoles"/>
                </resource>
                <resource id="domainIDGroupIDRoleID" path="{role_id}">
                    <param name="role_id" style="template" type="xsd:string" required="true">
                        <doc>Role to be acted upon</doc>
                    </param>
                    <method href="#grantDomainGroupRole"/>
                    <method href="#checkDomainGroupRole"/>
                    <method href="#revokeDomainGroupRole"/>
                </resource>
            </resource>
        </resource>
        
        <resource id="projects" path="//v3/projects">
            <method href="#listProjects"/>
            <method href="#createProject"/>
            <resource id="projectID" path="{project_id}">
                <param name="project_id" style="template" type="xsd:string" required="true">
                    <doc>Project to be acted upon</doc>
                </param>
                <method href="#getProject"/>
                <method href="#updateProject"/>
                <method href="#deleteProject"/>
                <resource id="projectIDUsers" path="users">
                    <method href="#listProjectUsers"/>
                    <resource id="projectIDUserIDRoles" path="{user_id}/roles">
                        <param name="user_id" style="template" type="xsd:string" required="true">
                            <doc>User to be acted upon</doc>
                        </param>
